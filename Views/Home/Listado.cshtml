@model IEnumerable<Remesa>

<table>
    <thead>
        <tr>
            <th>Remitente</th>
            <th>Destinatario</th>
            <th>País de Origen</th>
            <th>País de Destino</th>
            <th>Monto Enviado</th>
            <th>Moneda</th>
            <th>Tasa de Cambio</th>
            <th>Monto Final</th>
            <th>Estado</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var remesa in Model)
    {
        <tr>
            <td>@remesa.Remitente</td>
            <td>@remesa.Destinatario</td>
            <td>@remesa.PaisOrigen</td>
            <td>@remesa.PaisDestino</td>
            <td>@remesa.MontoEnviado</td>
            <td>@remesa.Moneda</td>
            <td>@remesa.TasaCambio</td>
            <td>@remesa.MontoFinal</td>
            <td>@remesa.Estado</td>
        </tr>
    }
    </tbody>
</table>
